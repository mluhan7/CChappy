/**app.wxss**/
@import "colorui/main.wxss";
@import "colorui/icon.wxss";


/*自定义通用*/
.bg-trans {
  background: transparent;
}

.text-title {
  position: relative;
  top: 20rpx;
}

.my-bg-white {
  background: #f6f9ff;
}

.my-bg-gray {
  background: #b3b3c3;
}

.my-bg-green {
  background: #4cd5a7;
}

.my-text-green {
  color: #4cd5a7;
}

.my-text-gray {
  color: #b3b3c3;
}

.text-motto {
  position: relative;
  bottom: 13rpx;

}

.myshadow {
  box-shadow: -1rpx 30rpx 40rpx 10rpx #e1e5f0;
}

.bgimg-cover {
  background-size: cover;
}

.zoomImage {
  background-image: url("http://www.triple2.xyz:8088/3.jpg");
  background-repeat: no-repeat;
  background-size: 100%;
}

.multi-ellipsis--l4 {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 4;
  -webkit-box-orient: vertical;
}


.pos-fixed {
  position: fixed;
}

.myicons {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
}

.myemptyhint {
  display: flex;
  justify-content: center;
}
