/* pages/indHome/indHome.wxss */
.UCenter-bg {
    background: url('data:image/svg+xml;base64,');
    background-size: cover;
    background-repeat: no-repeat;
    background-position: -5rpx;
    /* height: 550rpx; */
    display: flex;
    justify-content: center;
    overflow: hidden;
    position: relative;
    flex-direction: column;
    align-items: center;
    color:#d0cde1;
    font-weight: 300;
    text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}

.UCenter-bg text {
    opacity: 0.8;
}

.UCenter-bg image {
    width: 200rpx;
    height: 200rpx;
}

.UCenter-bg .gif-wave{
    position: absolute;
    width: 100%;
    bottom: -5rpx;
    left: 0;
    z-index: 99;
    mix-blend-mode: screen;
    height: 100rpx;
}

map,.mapBox{
    left: 0;
    z-index: 99;
    mix-blend-mode: screen;
    height: 100rpx;
}

map,.mapBox{
    width: 750rpx;
    height: 300rpx;
}

.avatar {
    width: 146rpx;
    height: 146rpx;
}

.mytabs {
    margin-bottom:30rpx;
}

.mytag {
    margin-right: 18rpx;
}